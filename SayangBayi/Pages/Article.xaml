      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SayangBayi.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Article">
    <Viewbox>
        <Grid Background="White">
            <Label Content="Articles" FontWeight="Bold" Margin="28,10,250,157" FontSize="16"/>
            <ListBox HorizontalAlignment="Left" Height="126" Margin="28,46,0,0" VerticalAlignment="Top" Width="286" d:ItemsSource="{d:SampleData ItemCount=5}"/>

        </Grid>
    </Viewbox>
    
</Page>
